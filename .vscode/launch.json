    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Godo run",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_godot",
            "program": "C:/Users/ericx/OneDrive/Desktop/Godot4/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe", //C:/Users/ericx/OneDrive/Desktop/Godot4/Godot_v4.2.1-stable_mono_win64
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": ["--path", "${workspaceRoot}"]
        },
        {
            "name": "Play in Editor",
            "type": "godot-mono",
            "mode": "playInEditor",
            "request": "launch"
        },
        {
            "name": "Launch",
            "type": "godot-mono",
            "request": "launch",
            "mode": "executable",
            "preLaunchTask": "build",
            "executable": "C:/Users/ericx/OneDrive/Desktop/Godot4/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
            "executableArguments": [
                "--path",
                "${workspaceRoot}"
            ]
        },
        {
            "name": "Launch (Select Scene)",
            "type": "godot-mono",
            "request": "launch",
            "mode": "executable",
            "preLaunchTask": "build",
            "executable": "C:/Users/ericx/OneDrive/Desktop/Godot4/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
            "executableArguments": [
                "--path",
                "${workspaceRoot}",
                "${command:SelectLaunchScene}"
            ]
        },
        {
            "name": "Attach",
            "type": "godot-mono",
            "request": "attach",
            "address": "localhost",
            "port": 23685
        }
    ]
}